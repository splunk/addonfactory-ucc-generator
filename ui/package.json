    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "typescript": "^5.2.2",
    "url": "^0.11.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "*.{jsx,js,tsx,ts}": [
      "eslint --fix"
    ],
    "*.{json, html, css}": [
      "prettier --write"
    ]
  },
  "resolutions": {
